# Starknet Network Configuration
# ⚠️ NUNCA hagas commit del archivo .env real con valores sensibles

# Network: 'mainnet' o 'testnet'
VITE_STARKNET_NETWORK=mainnet

# Payment Recipient Address (Mainnet)
# La dirección de tu wallet que recibirá los pagos
VITE_PAYMENT_RECIPIENT_MAINNET=0xYOUR_MAINNET_ADDRESS_HERE

# Payment Recipient Address (Testnet)
VITE_PAYMENT_RECIPIENT_TESTNET=0xYOUR_TESTNET_ADDRESS_HERE

# Oracle/Price Feed (opcional)
# Si usas un servicio de precios externo
VITE_PRICE_ORACLE_API_KEY=your_api_key_here

# ETH Price (fallback si no usas oracle)
VITE_ETH_PRICE_USD=2500
